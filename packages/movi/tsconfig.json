{
	"compilerOptions": {
		"sourceMap": true,
		"target": "ES6",
		"module": "ESNext",
		"moduleResolution": "Node",
		"allowJs": true,
		"strict": true,
		"noUnusedLocals": false,
		"experimentalDecorators": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"removeComments": false,
		"noImplicitAny": false,
		"noImplicitReturns": false,
		"noImplicitOverride": false,
		"declaration": true,
		"jsx": "preserve",
		"outDir": "./dist",
		"lib": [
			"esnext",
			"dom"
		],
		"types": [
		 
		],
	},
	"include": [
		"src/global.d.ts",
		"./src/**/*", 
		"global.d.ts",
		"global.d.ts"
	]
}